<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.diagnostics>
    <switches>
      <add name="switch" value="4"/>
    </switches>
    <trace autoflush="true" indentsize="2">
      <listeners>
        <add name="listener" type="System.Diagnostics.TextWriterTraceListener" initializeData="log.log"/>
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>